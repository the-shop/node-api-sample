{
  "name": "node-api-sample",
  "description": "node-api-sample by The Shop",
  "version": "0.1.0",
  "license": "UNLICENSED",
  "scripts": {
    "start": "npm-run-all --parallel start:api start:cron build:admin",
    "start:live": "npm-run-all --parallel run:live build:admin:prod",
    "run:live": "npm-run-all --parallel start:api:prod start:cron:prod",
    "start:api": "DEBUG=node-api-sample* nodemon --watch ./src ./node_modules/.bin/babel-node tools/apiStart.js",
    "start:api:prod": "DEBUG=node-api-sample* forever --minUptime=1000 --spinSleepTime=1000 ./node_modules/.bin/babel-node tools/apiStart.js",
    "build:admin": "DEBUG=node-api-sample* nodemon --watch ./admin admin/build.js",
    "build:admin:prod": "DEBUG=node-api-sample* node admin/build.js",
    "start:cron": "DEBUG=node-api-sample* nodemon --watch ./src/cron ./node_modules/.bin/babel-node tools/cronStart.js",
    "start:cron:prod": "DEBUG=node-api-sample* forever --minUptime=1000 --spinSleepTime=1000 ./node_modules/.bin/babel-node tools/cronStart.js",
    "test": "npm run lint && npm run unit:test",
    "lint": "./node_modules/.bin/eslint -c .eslintrc src/ tools/ admin/",
    "unit:test": "export NODE_ENV=test && export MONGODB_URI=mongodb://localhost/node-api-sample-test && ./node_modules/.bin/babel-node ./tools/unitTestRunner.js",
    "database": "DEBUG=node-api-sample* ./node_modules/.bin/babel-node ./src/Database/runner.js"
  },
  "pre-commit.silent": false,
  "pre-commit.colors": true,
  "pre-commit": [
    "test"
  ],
  "dependencies": {
    "admin-on-rest": "^1.3.1",
